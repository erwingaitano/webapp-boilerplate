// Use this file as a starting point for your project's .eslintrc.
// Copy this file, and add rule overrides as needed.
{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "no-console": 0,
    "no-shadow": 0,
    "arrow-body-style": 0,
    "comma-dangle": [2, "never"],
    "react/jsx-no-bind": 0,
    "no-param-reassign": 0
  }
}
